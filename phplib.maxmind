<VirtualHost *:80>
    DocumentRoot /vagrant
    ServerName phplib.maxmind
    
    php_value display_errors "1"
    php_value error_reporting -1

    <IfModule mod_geoip.c>
        GeoIPEnable Off
    </IfModule>

    <Directory /vagrant>
        Order Allow,Deny
        Allow from all

        DirectoryIndex lib.php
    </Directory>
</VirtualHost>
